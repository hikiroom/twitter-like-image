{
  "name": "twitter-like-image-viewer",
  "version": "1.0.2",
  "description": "Twitter風の画像ビューアー",
  "main": "index.js",
  "scripts": {
    "ts": "tsc",
    "ts:w": "tsc -w",
    "scss": "sass ./_dev/scss/TLImageViewer.scss ./_dev/css/TLImageViewer.css --no-source-map",
    "scss:w": "sass ./_dev/scss/TLImageViewer.scss ./_dev/css/TLImageViewer.css --no-source-map --watch",
    "dev": "run-p ts:w scss:w",
    "move": "node move.js",
    "build": "run-s ts scss move"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "minify": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.7",
    "typescript": "^3.9.3"
  }
}
